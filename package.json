{
  "name": "tailwindui-syntax",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "example": "vite --config vite-example.config.js",
    "clean": "rm -rf pages/*/",
    "dev": "spago build && vike dev",
    "gitpod": "spago build",
    "format": "purs-tidy format-in-place src/**/*.purs && prettier --write ./src/ vite-example.config.js vite-plugin-vike-deku.js vite.config.js gen-examples.js",
    "build": "node gen-examples.js && spago build && vike build",
    "prepare": "husky"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@eslint/compat": "^1.2.7",
    "@mikesol/wc-discord-message": "^3.0.2",
    "@sindresorhus/slugify": "^2.2.1",
    "@tailwindcss/postcss": "^4.0.16",
    "@tailwindcss/typography": "^0.5.16",
    "clipboard": "^2.0.11",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "flowbite": "^3.1.2",
    "focus-visible": "^5.2.1",
    "htmlparser2": "^10.0.0",
    "inflection": "^3.0.2",
    "js-confetti": "^0.12.0",
    "postcss-focus-visible": "^10.0.1",
    "prismjs": "^1.30.0",
    "sweetalert2": "^11.17.2",
    "tailwindcss": "^4.0.16",
    "toastify-js": "^1.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@vite-plugin-vercel/vike": "^9.0.4",
    "chokidar": "^4.0.3",
    "eslint": "^9.23.0",
    "glob": "^11.0.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^15.5.0",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "purs-tidy": "^0.11.0",
    "sharp": "^0.33.5",
    "svgo": "^3.3.2",
    "vike": "^0.4.227",
    "vite": "^6.2.3",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-plugin-vercel": "^9.0.5"
  }
}
